{
  "dart.flutterSdkPath": ".fvm/versions/3.19.6",
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "window.zoomLevel": -1,
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 16,
  "debug.toolBarLocation": "commandCenter",
  "workbench.activityBar.orientation": "vertical",
  "editor.codeActionsOnSave": {
    "source.fixall.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.mouseWheelZoom": false,
  "editor.accessibilitySupport": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "vscode-icons",
  "search.exclude": {
    ".fvm/": true
  },
  "files.trimTrailingWhitespace": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart, ${capture}.freezed.dart, ${capture}.flutterfire_gen.dart,"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.format.enable": true,
  "eslint.workingDirectories": [
    "./functions"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": false
  }
}